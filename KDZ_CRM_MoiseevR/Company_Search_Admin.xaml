<Window x:Class="KDZ_CRM_MoiseevR.Company_Search_Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KDZ_CRM_MoiseevR"
        mc:Ignorable="d"
        Title="Company_Search" Height="300" Width="300" Loaded="Window_Loaded">
    <Window.Resources>
        <local:CRM_DatabaseDataSet x:Key="cRM_DatabaseDataSet"/>
        <CollectionViewSource x:Key="company_ProtectedViewSource" Source="{Binding Company_Protected, Source={StaticResource cRM_DatabaseDataSet}}"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource company_ProtectedViewSource}">
        <Button x:Name="Back_Menu" Content="Назад в меню" HorizontalAlignment="Left" Margin="193,197,0,0" VerticalAlignment="Top" Width="86" Click="Back_Menu_Click"/>
        <Frame Name="FrameCompanySearch_Admin" NavigationUIVisibility="Hidden" Margin="5,50,0,135">

        </Frame>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="6,12,0,0" VerticalAlignment="Top" Width="276" Height="157" SelectionChanged="dataGrid_SelectionChanged" ItemsSource="{Binding}"/>
    </Grid>
</Window>
